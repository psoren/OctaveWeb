{
  "name": "syncify",
  "version": "1.0.0",
  "description": "Listen to songs together",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "dev": "NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Parker Sorenson",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.18.11"
  },
  "engines": {
    "node": "12.0.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.2",
    "http": "0.0.0",
    "http-proxy-middleware": "^0.19.1",
    "mongoose": "^5.5.1",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "shortid": "^2.2.14",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "toasted-notes": "^2.1.5"
  }
}